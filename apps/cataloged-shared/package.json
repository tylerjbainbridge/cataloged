{
  "name": "cataloged-shared",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "refresh:types": "yarn run remove:types && yarn run generate:types",
    "generate:types": "npx apollo codegen:generate --target=typescript --includes=\"src/**/*.ts{,x}\" --endpoint=http://localhost:5000 --globalTypesFile=graphql/__generated__/apolloTypes.ts --outputFlat=graphql/__generated__",
    "remove:types": "find . | grep -E \"(__generated__)\" | xargs rm -rf"
  },
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.39",
    "@apollo/link-context": "2.0.0-beta.3",
    "@chakra-ui/core": "^0.7.0",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@reach/alert": "^0.9.1",
    "@types/lodash": "^4.14.137",
    "@types/react": "^16.8.23",
    "apollo-boost": "^0.4.3",
    "apollo-cache-persist": "^0.1.1",
    "apollo-upload-client": "^11.0.0",
    "clean-deep": "^3.3.0",
    "cuid": "^2.1.8",
    "emotion-theming": "^10.0.27",
    "lodash": "^4.17.15",
    "mousetrap": "^1.6.5",
    "query-string": "^6.11.1",
    "react": "^0.0.0-235a6c4af",
    "react-dom": "^0.0.0-235a6c4af",
    "react-icons": "^3.9.0",
    "react-router-dom": "^6.0.0-alpha.2",
    "scroll-into-view-if-needed": "^2.2.24"
  },
  "devDependencies": {
    "apollo-codegen": "^0.20.2"
  }
}
