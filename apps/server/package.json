{
  "name": "cataloged-server",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "scripts": {
    "start:api": "node dist/api-server-entry.js",
    "build:api": "npx parcel build src/api-server-entry.ts --target node",
    "prebuild:api": "npx prisma2 generate",
    "start:worker": "node dist/worker-server-entry.js",
    "build:worker": "npx parcel build src/worker-server-entry.ts --target node",
    "prebuild:worker": "npx prisma2 generate",
    "dev": "npm-run-all --parallel dev:*",
    "dev:api": "npx nodemon --watch '*.ts{,x}' --exec 'npx ts-node -T' src/api-server-entry.ts",
    "dev:worker": "npx nodemon --watch '*.ts{,x}' --exec 'npx ts-node -T' src/worker-server-entry.ts",
    "predev": "npm run generate:imports",
    "prisma:generate": "npx prisma2 generate",
    "prisma:lift:up": "npx prisma2 lift up",
    "prisma:lift:save": "npx prisma2 lift save",
    "postinstall": "bash ./generate.sh",
    "generate:imports": "node src/generateImports.js",
    "seed": "ts-node prisma/seed.ts",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "resolutions": {
    "agent-base": "^5.0.0"
  },
  "dependencies": {
    "@babel/compat-data": "^7.8.0",
    "@babel/preset-env": "^7.8.7",
    "@prisma/client": "2.0.0-preview024",
    "@sendgrid/mail": "^6.5.4",
    "@sentry/node": "^5.12.3",
    "@types/react": "^16.9.23",
    "apollo-server-express": "^2.9.0",
    "aws-sdk": "^2.509.0",
    "axios": "^0.19.0",
    "base64url": "^3.0.1",
    "bluebird": "^3.5.5",
    "bull": "^3.13.0",
    "dotenv": "^8.0.0",
    "encoding": "^0.1.12",
    "file-type": "^14.1.3",
    "googleapis": "^42.0.0",
    "googleapis-common": "^3.0.0",
    "graphql": "^14.4.2",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.0.0",
    "graphql-upload": "^8.0.7",
    "jsdom": "^16.2.1",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.19.1",
    "lodash": "^4.17.15",
    "metascraper": "^5.9.4",
    "metascraper-author": "^5.8.13",
    "metascraper-clearbit": "^5.8.15",
    "metascraper-date": "^5.8.13",
    "metascraper-description": "^5.8.15",
    "metascraper-image": "^5.9.4",
    "metascraper-logo": "^5.8.13",
    "metascraper-publisher": "^5.8.13",
    "metascraper-title": "^5.8.13",
    "metascraper-url": "^5.8.13",
    "morgan": "^1.9.1",
    "nexus": "^0.12.0-rc.9",
    "nexus-prisma": "^0.11.1",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "pg": "^7.12.0",
    "prisma2": "2.0.0-preview024",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-html-email": "^3.0.0",
    "sharp": "^0.23.0",
    "simple-jwt": "^1.0.3",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.8.3",
    "@types/aws-lambda": "^8.10.31",
    "@types/bluebird": "^3.5.27",
    "@types/bull": "^3.12.1",
    "@types/graphql-upload": "^8.0.1",
    "@types/jest": "^24.0.16",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/lodash": "^4.14.149",
    "@types/memorystream": "^0.3.0",
    "@types/morgan": "^1.7.37",
    "@types/node": "^12.7.2",
    "@types/sharp": "^0.22.2",
    "jest": "^25.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^1.18.2",
    "ts-node": "^8.3.0",
    "ts-node-dev": "1.0.0-pre.40"
  }
}
